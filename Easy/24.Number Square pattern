public class NumberSquarePattern {
    public static void main(String[] args) {
        int size = 4; // Change this for desired size
        for (int i = 1; i <= size; i++) {
            for (int j = 1; j <= size; j++) {
                System.out.print(i + " ");
            }
            System.out.println();
        }
    }
}
